{
    "firestore": {
      "rules": "firestore/firestore.rules",
      "indexes": "firestore/firestore.indexes.json"
    },
    "storage": {
      "rules": "firestore/storage.rules"
    },
    "hosting": {
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
  